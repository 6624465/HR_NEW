<body class="hold-transition skin-blue sidebar-mini" ng-controller="layOutController as layOutcntrl">
    @*sidebar-mini*@
    <div class="wrapper" style="background:#fff !important;">
        <header class="main-header">
            <a  class="logo">
               @* <span class="logo-mini"><b>EZY-HR</b></span>*@
                <span class="logo-lg" ng-click="onLogoClick()" ><i aria-hidden="true" ></i>&nbsp;EZY-HR</span>
            </a>
                <nav class="navbar navbar-static-top" role="navigation">
                    @*<a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                        <span class="sr-only">Toggle navigation</span>
                    </a>*@
             @*   <span style="padding-top: 15px; color: #fff;" class="pull-left">Loggedin Branch  <b style="font-size: 15px;"></b></span>*@

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="hidden-xs">

                                    <i class="zmdi zmdi-face zmdi-hc-lg"></i> &nbsp; {{User.UserID }}  &nbsp;  <i class="fa fa-caret-down" aria-hidden="true"></i>

                                </span>
                            </a>
                            <ul class="dropdown-menu ">

                                <li class="user-footer user-other">

                                    <a href="#" class=""> <i class="fa fa-id-badge" aria-hidden="true"></i> &nbsp;Profile</a>

                                </li>
                                <li class="user-footer user-other">

                                    <a href="#" class=""><i class="zmdi zmdi-settings zmdi-hc-lg"></i> &nbsp; Setting</a>

                                </li>
                                <li class="user-footer user-other">
                                    <a href="#" class="" ng-click="onClickLogOut()"> <i class="zmdi zmdi-power zmdi-hc-lg"></i> &nbsp; LogOut</a>

                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
       
            <section class="content-wrapper"  >
                <div id="content" class="content-header" style="background-color:#fff;min-height:{{iframeHeight + 'px'}}"  ><data ui-view >@RenderBody()  </data></div></section>
      
        
        <footer class="main-footer">
            <div class="row">
                <div class="col-md-6">
                    <div class="hidden-xs">
                        <b>Version</b> 1.0.0
                    </div>
                </div>
                <div class="col-md-6 text-right">
                    <p>&copy; 2017 - <a href="#">Ezy-HR</a> </p>
                </div>
            </div>
        </footer>


    </div>

    <div id="myModalTest" class="modal fade" role="dialog">
        <div id="myModalTestInnerDiv" class="modal modal-success">
            <div class="modal-dialog modal-sm dialog-Meassage">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 id="modalHeader" class="modal-title" style="color:white">Modal Header</h4>
                    </div>
                    <div class="modal-body dialog-Meassage-body">
                        <p id="pmodalid">Records saved successfully.</p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" onclick="BtnOK()" data-dismiss="modal">OK</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="myModalTest2" class="modal fade" role="dialog">
        <div id="myModalTestInnerDiv2" class="modal modal-success">
            <div class="modal-dialog modal-sm dialog-Meassage">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 id="modalHeader2" class="modal-title" style="color:white">Modal Header</h4>
                    </div>
                    <div class="modal-body dialog-Meassage-body">
                        <p id="pmodalid2">Records saved successfully.</p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">OK</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="myModalDialog" class="modal fade" role="dialog">
        <div id="confirmDialog" class="modal modal-warning">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button class="close" aria-label="Close" data-dismiss="modal" type="button">&times;</button>
                        <h4 class="modal-title" style="color:white">ALERT!</h4>
                    </div>
                    <div class="modal-body">
                        <p id="pWarning">Do you want to Save the Record?</p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" onclick="BtnConfirmOK()" data-dismiss="modal">OK</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- script files -->
