    <script src="@Url.Content("~/dist/js/adminlte.min.js")"></script>
    <script src="@Url.Content("~/dist/js/pages/dashboard.js")"></script>
    <script src="@Url.Content("~/scripts/angular.js")"></script>
    <script src="@Url.Content("~/scripts/datetimepicker.js")"></script>
    <script src="@Url.Content("~/scripts/dateTimeInput.js")"></script>
    <script src="@Url.Content("~/scripts/app.js")"></script>
    <script src="@Url.Content("~/scripts/angular-ui-router.js")"></script>
