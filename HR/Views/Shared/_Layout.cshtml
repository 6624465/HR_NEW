    <script src="@Url.Content("~/scripts/ui-bootstrap.js")"></script>
    <script src="@Url.Content("~/scripts/ui-bootstrap-tpls.min.js")"></script>
    <script src="@Url.Content("~/scripts/loading-bar.js")"></script>
    <script src="@Url.Content("~/scripts/angular-animate.js")"></script>
    <script src="@Url.Content("~/scripts/ocLazyLoad.js")"></script>
    <script src="@Url.Content("~/scripts/ng-table.min.js")"></script>
    <script src="@Url.Content("~/scripts/angular-tree-control.js")"></script>
    <script src="@Url.Content("~/scripts/angular-aria.js")"></script>
    <script src="@Url.Content("~/scripts/angular-material.min.js")"></script>
    <script src="@Url.Content("~/scripts/angular-growl.js")"></script>
    <script src="@Url.Content("~/scripts/services.js")"></script>
    <script src="@Url.Content("~/scripts/bootstrap-growl.min.js")"></script>
    <script src="@Url.Content("~/scripts/bootstrap-growl.js")"></script>
    <!-- Select2 -->
    <script src="@Url.Content("~/bower_components/select2/dist/js/select2.full.min.js")"></script>
    <!-- InputMask -->
    <script src="@Url.Content("~/plugins/input-mask/jquery.inputmask.js")"></script>
    <script src="@Url.Content("~/plugins/input-mask/jquery.inputmask.date.extensions.js")"></script>
    <script src="@Url.Content("~/plugins/input-mask/jquery.inputmask.extensions.js")"></script>
    <script src="@Url.Content("~/plugins/iCheck/icheck.min.js")"></script>
   
    <!-- End Script files -->
    <script type="text/javascript">
        var baseUrl = '@Url.Content("~/")';
        angular.module('ngHR').controller('layOutController', ['$scope', '$http', 'growlService', '$window', '$location', '$state', function ($scope, $http, growlService, $window, $location,$state) {
            $scope.init = function () {
                $scope.HomeUrl = $location.absUrl();
                $scope.iframeHeight = window.innerHeight - 109;
                $scope.User = JSON.parse(sessionStorage.getItem("authenticatedUser"));
                var Ismessagedisplayed = sessionStorage.getItem('Ismessagedisplayed') == 'true' ? true : false;
                if (!Ismessagedisplayed && Ismessagedisplayed != undefined) {
                    sessionStorage.setItem('Ismessagedisplayed', 'true');
                    growlService.growl("Welcome To Hr", 'success');
                }
            }
            $scope.onClickLogOut = function () {
                $http.get("/Login/LogOut").then(function (res) {
                    if (res.data && res.data.success == true) {
                        angular.forEach(sessionStorage, function (value, key) {
                            sessionStorage.removeItem(key);
                        });
                        window.location.pathname = '';
                    }
                })
            }

            $scope.onLogoClick = function () {
                //location.href = baseUrl + '/Home/index';
                $state.go();
                //location.href = $scope.HomeUrl;
            }
            $scope.init();

        }]);
        //iCheck for checkbox and radio inputs
        $('input[type="checkbox"].minimal, input[type="radio"].minimal').iCheck({
           
            checkboxClass: 'icheckbox_minimal-blue',
            radioClass: 'iradio_minimal-blue'

        })
        //Red color scheme for iCheck
        $('input[type="checkbox"].minimal-red, input[type="radio"].minimal-red').iCheck({
            
            checkboxClass: 'icheckbox_minimal-red',
            radioClass: 'iradio_minimal-red'
        })
        //Flat red color scheme for iCheck
        $('input[type="checkbox"].flat-red, input[type="radio"].flat-red').iCheck({
            checkboxClass: 'icheckbox_flat-green',
            radioClass: 'iradio_flat-green'
        })

    </script>
