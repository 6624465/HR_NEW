<style>
    .highcharts-credits {
        display: none;
    }

    .highcharts-data-label-connector {
        display: none;
    }

    .spanboxNumber {
        position: absolute;
        right: 205px;
        width: 3px;
        border: 5px solid #337ef7;
        padding: 3px;
        border-radius: 50%;
        background-color: #337ef7;
    }

    .spanboxNumber2 {
        position: absolute;
        right: 105px;
        width: 3px;
        border: 5px solid #ff66b3;
        padding: 3px;
        border-radius: 50%;
        background-color: #ff66b3;
    }

    .spanClass {
        position: absolute;
        padding-left: 20px;
        line-height: 2px;
    }
</style>
